<Window x:Class="GeographyApp.MainWindow" WindowStyle="None" Background="LightBlue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeographyApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="GeolanderTitleCanvas" Height="auto" Width="auto">
            <Image x:Name="GeolanderTitle" Margin="176,52,176,321" Source="/Photos/geolanderTitle.png" Stretch="Fill"/>
        </Canvas>
        <StackPanel Margin="125, 200, 125, 125" Orientation="Vertical" Name="MainMenuUserControls" Opacity="0">
            <Button Content="Play" Margin="10"></Button>
            <Button Content="Info" Margin="10"></Button>
            <Button Content="Exit" Margin="10"></Button>
        </StackPanel>
    </Grid>
</Window>
